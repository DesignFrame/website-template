{
  "name": "dfs/project",
  "description": "DFS custom website",
  "authors": [
    {
      "name": "Alex Standiford",
      "email": "alex@designframesolutions.com"
    }
  ],
  "repositories":[
    {
      "type":"composer",
      "url":"https://wpackagist.org",
      "only": [
        "wpackagist-plugin/*",
        "wpackagist-theme/*"
      ]
    },
    {
      "type": "vcs",
      "url": "git@github.com:DesignFrame-Registry/edd-recurring",
      "reference": "master"
    }
  ],
  "extra": {
    "installer-paths": {
      "wp-content/plugins/{$name}/": ["type:wpackagist-plugin", "type:wordpress-plugin"],
      "wp-content/themes/{$name}/": ["type:wpackagist-theme","type:wordpress-theme"],
      "wp-content/mu-plugins/vendor/{$vendor}/{$name}": ["type:wordpress-muplugin"]
    }
  },
  "require": {
    "alexstandiford/underpin": "^1.1",
    "designframe/edd-recurring-plugin": "dev-master",
    "wpackagist-plugin/easy-digital-downloads": "*"
  }
}
